.title {
  position: relative;
  text-align: center;
  padding-top: 20rpx;
  font-size: 28rpx;
  color: #333;
}

.most-expected {
  position: relative;
  padding: 0 30rpx;
  padding-bottom: 20rpx;
  border-bottom: 20rpx solid #f5f5f5;
}

.box {
  position: relative;
  text-align: center;
  padding-top: 20rpx;
  color: #333;
}

.box-item {
  padding-top: 20rpx;
  font-size: 20rpx;
  color: #f03d37;
}

.box-info {
  font-size: 60rpx;
  color: #f03d37;
}

.box-update-time {
  padding-top: 20rpx;
  font-size: 20rpx;
  color: #333;
}

.ranking {
  padding: 0 15rpx;
  padding-top: 20rpx;
  font-size: 32rpx;
  color: #333;
}

.table {
  border: 0px solid darkgray;
  font-size: 12px;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: left;
  height: 3rem;
  align-items: center;
}

.td {
  padding: 0 15rpx;
  width: 50%;
  justify-content: center;
  text-align: center;
}

.bg-w{
  /* background: snow; */
}

.bg-g{
  background: #F8F8F8;
}

.th {
  width: 40%;
  justify-content: center;
  background: #F0F0F0;
  display: flex;
  height: 2rem;
  align-items: center;
}

.box-infos {
  color: #f03d37;
}
